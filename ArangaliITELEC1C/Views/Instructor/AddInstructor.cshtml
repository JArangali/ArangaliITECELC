@model Instructor;

<form asp-action="AddInstructor" asp-controller="Instructor" method="post">
    <div class="form-group">
        <label asp-for="InstructorFirstName">First Name</label>
        <input asp-for="InstructorFirstName" class="form-control">
    </div>

    <div class="form-group">
        <label asp-for="InstructorLastName">Last Name</label>
        <input asp-for="InstructorLastName" class="form-control">
    </div>

    <div class="form-group">
        <label asp-for="InstructorIsTenured">InstructorIsTenured</label>
        <select asp-for="@Model.InstructorIsTenured" class="form-control"></select>
    </div>

    <div class="form-group">
        <label asp-for="DateHired">AdmussionDate</label>
        <input asp-for="DateHired" class="form-control">
    </div>

    <div class="form-group">
        <label asp-for="Rank">Rank</label>
        <input asp-for="Rank" class="form-control"></>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
    <button type="button" onclick="history.back()" class="btn btn-secondary">Cancel</button>
</form>